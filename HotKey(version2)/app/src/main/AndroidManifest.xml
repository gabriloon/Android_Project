<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.hotkey">

    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/hotkey_icon2"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity"></activity>
        <activity android:name=".PreConnect">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ListMenu"></activity>
        <activity android:name=".SettingMenu"></activity>
        <activity android:name=".Word1"></activity>
        <activity android:name=".Excel1"></activity>
        <activity android:name=".Battle1"></activity>
        <activity android:name=".settingbattle"></activity>
        <activity android:name=".settingword"></activity>
        <activity android:name=".settingtouchpad"></activity>
        <activity android:name=".battleset"></activity>
        <activity android:name=".battleset2"></activity>
        <activity android:name=".battleset3"></activity>
        <activity android:name=".battleset4"></activity>
    </application>

</manifest>